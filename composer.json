{
    "type": "project",
    "name": "frzb/unblocker-service",
    "description": "The UnBlocker Service allows you get update of rules for your router to unblock it via VPN or other ISP",
    "keywords": [
        "project",
        "symfony",
        "messenger",
        "transactional",
        "transaction",
        "serialize",
        "serializer",
        "unblocker",
        "blocker",
        "ip",
        "frzb"
    ],
    "homepage": "https://github.com/fractalzombie/unblocker-service",
    "license": "MIT",
    "authors": [
        {
            "name": "Mykhailo Shtanko",
            "email": "fractalzombie@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-amqp": "*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/doctrine-bundle": "^2.8",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.14",
        "evilfreelancer/routeros-api-php": "^1.5",
        "fp4php/functional": "^v4.20",
        "frzb/dependency-injection": "^1.6",
        "frzb/request-mapper": "^1.10",
        "frzb/transactional-messenger": "^1.1",
        "happyr/doctrine-specification": "^2.0",
        "illuminate/collections": "^9.48",
        "nelmio/api-doc-bundle": "^4.11",
        "symfony/amqp-messenger": "6.2.*",
        "symfony/asset": "6.2.*",
        "symfony/clock": "6.2.*",
        "symfony/console": "6.2.*",
        "symfony/dotenv": "6.2.*",
        "symfony/expression-language": "6.2.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "6.2.*",
        "symfony/http-client": "6.2.*",
        "symfony/intl": "6.2.*",
        "symfony/messenger": "6.2.*",
        "symfony/monolog-bundle": "^3.8",
        "symfony/notifier": "6.2.*",
        "symfony/runtime": "6.2.*",
        "symfony/security-bundle": "6.2.*",
        "symfony/serializer": "6.2.*",
        "symfony/string": "6.2.*",
        "symfony/telegram-notifier": "6.2.*",
        "symfony/uid": "6.2.*",
        "symfony/ux-live-component": "^2.7",
        "symfony/ux-twig-component": "^2.7",
        "symfony/validator": "6.2.*",
        "symfony/webpack-encore-bundle": "^1.16",
        "symfony/yaml": "6.2.*"
    },
    "require-dev": {
        "composer/package-versions-deprecated": "^1.11",
        "friendsofphp/php-cs-fixer": "^3.14",
        "pestphp/pest": "^1.22",
        "pestphp/pest-plugin-mock": "^1.0",
        "roave/security-advisories": "dev-latest",
        "symfony/browser-kit": "6.2.*",
        "symfony/css-selector": "6.2.*",
        "symfony/maker-bundle": "^1.48",
        "symfony/phpunit-bridge": "6.2.*",
        "symfony/stopwatch": "6.2.*",
        "symfony/var-dumper": "6.2.*",
        "symfony/web-profiler-bundle": "6.2.*",
        "vimeo/psalm": "^5.7"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true,
            "pestphp/pest-plugin": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "UnBlockerService\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UnBlockerService\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.2.*"
        }
    }
}
